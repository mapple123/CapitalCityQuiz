<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.capital_city_quiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.capital_city_quiz.HighscoreActivity"  android:screenOrientation="portrait"></activity>
        <activity android:name="com.example.capital_city_quiz.EndeActivity"  android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.capital_city_quiz.Quiz"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.capital_city_quiz.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>